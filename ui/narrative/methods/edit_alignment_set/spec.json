{
   "name" : "Edit Media",
   "authors": ["pranjan77"],
   "ver" : "1.0.0",
   "contact" : "http://kbase.us/contact-us/",
   "visble" : true,
   "categories" : ["active"],
   "widgets" : {
     "input" : "kbaseNarrativeMethodInput",
     "output" : "no-display"
   },
   "parameters" : [
      {
         "text_options" : {
            "valid_ws_types" : [ "KBaseSets.ReadsAlignmentSet" ],
	    "is_output_name": false
         },
