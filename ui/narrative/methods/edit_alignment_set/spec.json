         "default_values" : [ "" ],
         "id" : "alignment_set_ref",
         "optional" : false,
         "allow_multiple" : false,
         "field_type" : "text"
      },
      {
         "default_values" : [
            "alignment_set"
         ],
         "advanced" : false,
         "id" : "output_object_name",
         "optional" : false,
         "allow_multiple" : false,
         "field_type" : "text",
         "text_options" : {
            "valid_ws_types" : [
               "KBaseSets.ReadsAlignmentSet"
            ],
            "is_output_name" : true
         }
      },
      {
         "field_type": "textsubdata",
         "allow_multiple": false,
         "optional": true,
         "id": "alignments_to_remove",
         "default_values": [
            ""
